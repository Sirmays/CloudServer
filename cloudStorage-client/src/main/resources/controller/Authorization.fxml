<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="440.0" prefWidth="646.0" style="-fx-background-color: #65748a"
            xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Authorization">
    <children>

        <Button layoutX="468.0" layoutY="21.0" mnemonicParsing="false" onAction="#register" prefHeight="43.0" prefWidth="163.0"
                text="Зарегистрироваться" textFill="#65748a" >
            <font>
                <Font name="System Italic" size="14.0"/>
            </font>
        </Button>
        <Button layoutX="285.0" layoutY="322.0" mnemonicParsing="false" onAction="#entrance" prefHeight="26.0" prefWidth="75.0" text="Вход"
                textFill="#5b7ba8">
            <font>
                <Font name="System Italic" size="12.0"/>
            </font>
        </Button>
        <TextField fx:id="login" layoutX="217.0" layoutY="206.0" prefHeight="26.0" prefWidth="212.0" promptText="Логин">
            <font>
                <Font size="15.0"/>
            </font>
        </TextField>
        <PasswordField fx:id="password" layoutX="217.0" layoutY="265.0" prefHeight="26.0" prefWidth="212.0" promptText="Пароль">
            <font>
                <Font size="15.0"/>
            </font>
        </PasswordField>
        <Label layoutX="217.0" layoutY="64.0" prefHeight="94.0" prefWidth="212.0" text="     InCloud"
               textFill="#f5f5f5">
            <font>
                <Font name="System Bold Italic" size="32.0"/>
            </font>
        </Label>

    </children>
</AnchorPane>
